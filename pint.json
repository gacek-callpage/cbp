{
    "preset": "laravel",
    "rules": {
        "single_blank_line_before_namespace": false,
        "blank_line_after_opening_tag": false,
        "binary_operator_spaces": {
            "operators": {
                "=>": "align"
            }
        },
        "cast_spaces": {
            "space": "none"
        },
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": false,
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "method": "one",
                "property": "one",
                "trait_import": "none",
                "case": "none"
            }
        },
        "phpdoc_align": {
            "align": "vertical"
        }
    }
}
